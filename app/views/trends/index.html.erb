<h1>Trends Analysis</h1>
<div id="chart"></div>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    const data = [30, 86, 168, 281, 303, 365];
    const chart = d3.select("#chart")
      .append("svg")
      .attr("width", 400)
      .attr("height", 100);

    chart.selectAll("rect")
      .data(data)
      .enter()
      .append("rect")
      .attr("width", d => d)
      .attr("height", 20)
      .attr("y", (d, i) => i * 25)
      .attr("fill", "steelblue");
  });
</script>
